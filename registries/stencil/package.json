{
  "name": "@xotoboil-multifront/stencil",
  "version": "0.0.0",
  "description": "Stencil Component Starter",
  "main": "dist/main.js",
  "types": "dist/__types__",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
